RewriteEngine On
RewriteRule ^v([0-9]{1,2})/(.*?)/(.*?)/([0-9]{1,2,3,4,5})/(raw|html|serial|json|xml).api ./index.php?version=$1&country=$2&place=$3&radius=$4&output=$5 [L,NC,QSA]
RewriteRule ^v([0-9]{1,2})/(.*?)/(.*?)/(raw|html|serial|json|xml).api ./index.php?version=$1&country=$2&place=$3&output=$4 [L,NC,QSA]
RewriteRule ^v([0-9]{1,2})/(nearby)/([0-9\.\-]+)/([0-9\.\-]+)/([0-9]{1,2,3,4,5})/(raw|html|serial|json|xml).api ./index.php?version=$1&country=$2&latitude=$3&longitude=$4&radius=$5&output=$6 [L,NC,QSA]
RewriteRule ^v([0-9]{1,2})/(list)/(.*?)/(raw|html|serial|json|xml).api ./index.php?version=$1&country=$2&place=$3&output=$4 [L,NC,QSA]

